{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters":{},
    "variables": {
        "vnetTemplateLink": "https://raw.githubusercontent.com/rohithah/logic-apps-samples/main/LAv2-vNet/templates/vnet.template.json",
        "vnetParameterLink": "https://raw.githubusercontent.com/rohithah/logic-apps-samples/main/LAv2-vNet/templates/vnet.parameters.json",
        "deploymentStorageTemplateLink": "https://raw.githubusercontent.com/rohithah/logic-apps-samples/main/LAv2-vNet/templates/deploymentStorage.template.json",
        "deploymentStorageParameterLink": "https://raw.githubusercontent.com/rohithah/logic-apps-samples/main/LAv2-vNet/templates/deploymentStorage.parameters.json",
        "storagePrivateEndpointTemplateLink": "https://raw.githubusercontent.com/rohithah/logic-apps-samples/main/LAv2-vNet/templates/storagePrivateEndpoint.template.json",
        "storagePrivateEndpointParameterLink": "https://raw.githubusercontent.com/rohithah/logic-apps-samples/main/LAv2-vNet/templates/storagePrivateEndpoint.parameters.json",
        "appInsightTemplateLink": "https://raw.githubusercontent.com/rohithah/logic-apps-samples/main/LAv2-vNet/templates/appInsight.template.json",
        "appInsightParameterLink": "https://raw.githubusercontent.com/rohithah/logic-apps-samples/main/LAv2-vNet/templates/appInsight.parameters.json",
        "logicAppFETemplateLink": "https://raw.githubusercontent.com/rohithah/logic-apps-samples/main/LAv2-vNet/templates/logicAppFE.template.json",
        "logicAppFEParameterLink": "https://raw.githubusercontent.com/rohithah/logic-apps-samples/main/LAv2-vNet/templates/logicAppFE.parameters.json",
        "logicAppBETemplateLink": "https://raw.githubusercontent.com/rohithah/logic-apps-samples/main/LAv2-vNet/templates/logicAppBE.template.json",
        "logicAppBEParameterLink": "https://raw.githubusercontent.com/rohithah/logic-apps-samples/main/LAv2-vNet/templates/logicAppBE.parameters.json"
    },
    "resources": [
        {
            "apiVersion": "2017-05-10",
            "name": "vnet",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('vnetTemplateLink')]"
                },
                "parametersLink": {
                    "uri": "[variables('vnetParameterLink')]"
                }
            }
        },
        {
            "apiVersion": "2017-05-10",
            "name": "appInsight",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('appInsightTemplateLink')]"
                },
                "parametersLink": {
                    "uri": "[variables('appInsightParameterLink')]"
                }
            }
        },
        {
            "apiVersion": "2017-05-10",
            "name": "deploymentStorage",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('deploymentStorageTemplateLink')]"
                },
                "parametersLink": {
                    "uri": "[variables('deploymentStorageParameterLink')]"
                }
            }
        },
        {
            "apiVersion": "2017-05-10",
            "name": "applicationStorage",
            "type": "Microsoft.Resources/deployments",
            "dependsOn": ["deploymentStorage", "vnet"],
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('storagePrivateEndpointTemplateLink')]"
                },
                "parametersLink": {
                    "uri": "[variables('storagePrivateEndpointParameterLink')]"
                }
            }
        },
        {
            "apiVersion": "2017-05-10",
            "name": "logicAppBE",
            "type": "Microsoft.Resources/deployments",
            "dependsOn": ["deploymentStorage", "vnet", "applicationStorage", "appInsight"],
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('logicAppBETemplateLink')]"
                },
                "parametersLink": {
                    "uri": "[variables('logicAppBEParameterLink')]"
                }
            }
        },
        {
            "apiVersion": "2017-05-10",
            "name": "logicAppFE",
            "type": "Microsoft.Resources/deployments",
            "dependsOn": ["deploymentStorage", "vnet", "applicationStorage", "appInsight"],
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('logicAppFETemplateLink')]"
                },
                "parametersLink": {
                    "uri": "[variables('logicAppFEParameterLink')]"
                }
            }
        }
    ]
}