{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "storageAccountName": {
            "value": "contosoappstate"
        },
        "storageAccountType": {
            "value": "Standard_LRS"
        },        
        "storageAccountKind": {
            "value": "StorageV2"
        },
        "storageAccountAccessTier": {
            "value": "Hot"
        },
        "storageAccountMinimumTlsVersion": {
            "value": "TLS1_2"
        },
        "storageAccountSupportsHttpsTrafficOnly": {
            "value": true
        },
        "storageAccountAllowBlobPublicAccess": {
            "value": true
        },
        "storageAccountAllowSharedKeyAccess": {
            "value": true
        },
        "storageAccountNetworkAclsBypass": {
            "value": "AzureServices"
        },
        "storageAccountNetworkAclsDefaultAction": {
            "value": "Deny"
        },
        "vNetName": {
            "value": "ContosoVNET"
        },        
        "subnetName": {
            "value": "private-links"
        },
        "privateEndpointName": {
            "value": "private-blob-endpoint"
        },
        "privateEndpointConnectionName": {
            "value": "private-blob-endpoint-connection"
        },
        "privateEndpointApiVersion": {
            "value": "2020-03-01"
        },
        "groupId": {
            "value": "blob"
        },
        "location": {
            "value": "westus2"
        },
        "tags": {
            "value": {}
        }
    }
}