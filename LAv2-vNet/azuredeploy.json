{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters":{},
    "variables": {
        "vnetTemplateLink": "https://raw.githubusercontent.com/rohithah/arm-templates/main/LAv2-vNet/vnet.template.json",
        "vnetParameterLink": "https://raw.githubusercontent.com/rohithah/arm-templates/main/LAv2-vNet/vnet.parameters.json",
        "deploymentStorageTemplateLink": "https://raw.githubusercontent.com/rohithah/arm-templates/main/LAv2-vNet/deploymentStorage.template.json",
        "deploymentStorageParameterLink": "https://raw.githubusercontent.com/rohithah/arm-templates/main/LAv2-vNet/deploymentStorage.parameters.json",
        "storagePrivateEndpointTemplateLink": "https://raw.githubusercontent.com/rohithah/arm-templates/main/LAv2-vNet/storagePrivateEndpoint..template.json",
        "storagePrivateEndpointParameterLink": "https://raw.githubusercontent.com/rohithah/arm-templates/main/LAv2-vNet/storagePrivateEndpoint..parameters.json"
    },
    "resources": [
        {
            "apiVersion": "2015-01-01",
            "name": "vnet",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('vnetTemplateLink')]"
                },
                "parametersLink": {
                    "uri": "[variables('vnetParameterLink')]"
                }
            }
        },
        {
            "apiVersion": "2015-01-01",
            "name": "deploymentStorage",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('deploymentStorageTemplateLink')]"
                },
                "parametersLink": {
                    "uri": "[variables('deploymentStorageParameterLink')]"
                }
            }
        },
        {
            "apiVersion": "2015-01-01",
            "name": "applicationStorage",
            "type": "Microsoft.Resources/deployments",
            "dependsOn": ["deploymentStorage", "vnet"],
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[variables('storagePrivateEndpointTemplateLink')]"
                },
                "parametersLink": {
                    "uri": "[variables('storagePrivateEndpointParameterLink')]"
                }
            }
        }
    ]
}